{
  "name": "toys-legend-discord-bot",
  "version": "0.0.1",
  "main": "src/index.ts",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@types/node": "^17.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.3",
    "nodemon": "^2.0.15",
    "pm2": "^5.1.2",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "scripts": {
    "start": "pm2 start ecosystem.config.cjs --watch --ignore-watch=\"node_modules\"",
    "stop": "pm2 stop ecosystem.config.cjs",
    "build": "tsc -p .",
    "dev": "nodemon",
    "deploy:commands": "yarn ts-node ./src/scripts/deployCommands.ts"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@discordjs/builders": "^0.9.0",
    "@discordjs/rest": "^0.1.0-canary.0",
    "discord-api-types": "^0.25.2",
    "discord.js": "^13.3.1",
    "dotenv": "^10.0.0",
    "ethers": "^5.5.2"
  }
}
